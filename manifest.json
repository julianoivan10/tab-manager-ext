{
    "manifest_version": 3,
    "name": "Tab Manager Extension",
    "version": "1.2.4",
    "description": "An advanced tab manager for organizing and managing browser tabs.",
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png"
      }
    },
    "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png"
    }
}
  